{
  "name": "weather",
  "version": "0.0.28",
  "description": "A weather app built with Quasar",
  "productName": "Quasar Weather",
  "cordovaId": "com.day41.weather",
  "capacitorId": "",
  "author": "johne2310 <john@day41.com.au>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ./",
    "test": "echo \"No test specified\" && exit 0",
